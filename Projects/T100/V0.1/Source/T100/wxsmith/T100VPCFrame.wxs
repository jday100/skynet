<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="T100VPCFrame">
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<object class="wxMenuBar" variable="MenuBar1" member="yes">
			<object class="wxMenu" variable="Menu1" member="yes">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="ID_MENUITEM_START" variable="StartMenu" member="yes">
					<label>Start</label>
					<handler function="OnStartMenuSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM_STOP" variable="StopMenu" member="yes">
					<label>Stop</label>
					<handler function="OnStopMenuSelected" entry="EVT_MENU" />
				</object>
				<object class="separator" />
				<object class="wxMenuItem" name="ID_MENUITEM_QUIT" variable="QuitMenu" member="yes">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuitMenuSelected" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu6" member="yes">
				<label>Debug</label>
				<object class="wxMenuItem" name="ID_MENUITEM_DEBUG" variable="DebugMenu" member="yes">
					<label>Debug</label>
					<handler function="OnDebugMenuSelected" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu8" member="yes">
				<label>Config</label>
				<object class="wxMenuItem" name="ID_MENUITEM_HARDWARE" variable="HardwareMenu" member="yes">
					<label>Hardware</label>
					<handler function="OnHardwareMenuSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM_SETUP" variable="SetupMenu" member="yes">
					<label>Setup</label>
					<handler function="OnSetupMenuSelected" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu11" member="yes">
				<label>Help</label>
				<object class="wxMenuItem" name="ID_MENUITEM_ABOUT" variable="AboutMenu" member="yes">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application </help>
					<handler function="OnAboutMenuSelected" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-10</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
