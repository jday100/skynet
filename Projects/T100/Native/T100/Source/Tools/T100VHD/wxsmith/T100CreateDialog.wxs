							<object class="sizeritem">
								<object class="wxBoxSizer" variable="BoxSizer3" member="no">
									<orient>wxVERTICAL</orient>
									<object class="sizeritem">
										<object class="wxRadioButton" name="ID_RADIOBUTTON_FILE_STORAGE_FIXED" variable="RadioButtonFileStorageFixed" member="yes">
											<label>Fixed</label>
											<handler function="OnRadioButtonFileStorageFixedSelect" entry="EVT_RADIOBUTTON" />
										</object>
										<flag>wxALL|wxALIGN_LEFT</flag>
										<border>5</border>
										<option>1</option>
									</object>
									<object class="sizeritem">
										<object class="wxRadioButton" name="ID_RADIOBUTTON_FILE_STORAGE_DYNAMIC" variable="RadioButtonFileStorageDynamic" member="yes">
											<label>Dynamic</label>
											<handler function="OnRadioButtonFileStorageDynamicSelect" entry="EVT_RADIOBUTTON" />
										</object>
										<flag>wxALL|wxALIGN_LEFT</flag>
										<border>5</border>
										<option>1</option>
									</object>
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
