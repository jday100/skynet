							<object class="sizeritem">
								<object class="wxBoxSizer" variable="BoxSizer2" member="no">
									<orient>wxVERTICAL</orient>
									<object class="sizeritem">
										<object class="wxRadioButton" name="ID_RADIOBUTTON_FILE_TYPE_VHD" variable="RadioButtonFileTypeVHD" member="yes">
											<label>VHD</label>
											<handler function="OnRadioButtonFileTypeVHDSelect" entry="EVT_RADIOBUTTON" />
										</object>
										<flag>wxALIGN_LEFT</flag>
									</object>
									<object class="sizeritem">
										<object class="wxRadioButton" name="ID_RADIOBUTTON_FILE_TYPE_VHDX" variable="RadioButtonFileTypeVHDX" member="yes">
											<label>VHDX</label>
											<handler function="OnRadioButtonFileTypeVHDXSelect" entry="EVT_RADIOBUTTON" />
										</object>
										<flag>wxALIGN_LEFT</flag>
									</object>
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
