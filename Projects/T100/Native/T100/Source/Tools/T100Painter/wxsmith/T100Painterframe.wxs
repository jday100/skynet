<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="T100PainterFrame">
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="ID_MENUITEM_NEW" variable="MenuItemNew" member="yes">
					<label>New</label>
					<handler function="OnMenuItemNewSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM_OPEN" variable="MenuItemOpen" member="yes">
					<label>Open</label>
					<handler function="OnMenuItemOpenSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM_CLOSE" variable="MenuItemClose" member="yes">
					<label>Close</label>
					<handler function="OnMenuItemCloseSelected" entry="EVT_MENU" />
				</object>
				<object class="separator" />
				<object class="wxMenuItem" name="ID_MENUITEM_SAVE" variable="MenuItemSave" member="yes">
					<label>Save</label>
					<handler function="OnMenuItemSaveSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM_SAVE_AS" variable="MenuItemSaveAs" member="yes">
					<label>Save As</label>
					<handler function="OnMenuItemSaveAsSelected" entry="EVT_MENU" />
				</object>
				<object class="separator" />
				<object class="wxMenuItem" name="ID_MENUITEM_QUIT" variable="MenuItemQuit" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu3" member="yes">
				<label>Edit</label>
				<object class="wxMenuItem" name="ID_MENUITEM_UNDO" variable="MenuItemUndo" member="yes">
					<label>Undo</label>
					<handler function="OnMenuItemUndoSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM_REDO" variable="MenuItemRedo" member="yes">
					<label>Redo</label>
					<handler function="OnMenuItemRedoSelected" entry="EVT_MENU" />
				</object>
				<object class="separator" />
				<object class="wxMenuItem" name="ID_MENUITEM_CUT" variable="MenuItemCut" member="yes">
					<label>Cut</label>
					<handler function="OnMenuItemCutSelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM_COPY" variable="MenuItemCopy" member="yes">
					<label>Copy</label>
					<handler function="OnMenuItemCopySelected" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM_PASTE" variable="MenuItemPaste" member="yes">
					<label>Paste</label>
					<handler function="OnMenuItemPasteSelected" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu4" member="yes">
				<label>View</label>
				<object class="wxMenuItem" name="ID_MENUITEM11" variable="MenuItem13" member="yes">
					<label>Toolbars</label>
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM12" variable="MenuItem14" member="yes">
					<label>Panels</label>
				</object>
			</object>
			<object class="wxMenu" variable="Menu5" member="yes">
				<label>Setting</label>
				<object class="wxMenuItem" name="ID_MENUITEM_CONFIG" variable="MenuItemConfig" member="yes">
					<label>Config</label>
					<handler function="OnMenuItemConfigSelected" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>2</fields>
			<widths>-1,-10</widths>
			<styles>wxSB_NORMAL,wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
