<div>
    Hello
</div>
<div>
    <div>
        <div>
            <label>Username:</label>
            <input disabled type="text" value="<%=identity.username%>">
        </div>
    </div>
</div>
<form id="form" onsubmit="return false;">
<input type="hidden" name="test">
<div>
    <div>
        <div>
            <label>Nickname:</label>
            <input type="text" name="nickname" value="<%=identity.nickname%>">
        </div>
    </div>
</div>
<div>
    <div>
        <div>
            <label>Location:</label>
        </div>
    </div>
    <div>
        <div>
            <dl>
                <dd>
                    <div class="region">
                        <label>Continent:</label>
                        <select style="display:block;width: 150px;" id="continent" name="continent" onchange="continent_change(this, 'region');">

                        </select>
                    </div>
                    
                    <div class="region">
                        <label>Region:</label>
                        <select style="display:block;width: 150px;" id="region" name="region" onchange="region_change(this, 'city');">
                            
                        </select>
                    </div>

                    <div class="region">
                        <label>City:</label>
                        <select style="display:block;width: 150px;" id="city" name="city">
                            
                        </select>
                    </div>
                </dd>
            </dl>
        </div>
    </div>
</div>
<div>
    <div>
        <div>
            <label>Intro:</label>
        </div>
    </div>
</div>
<div>
    <div>
        <div>
            <textarea name="intro" rows="10" cols="80"></textarea>
        </div>
    </div>
</div>
<div>
    <button id="submit">Save</button>
</div>
</form>