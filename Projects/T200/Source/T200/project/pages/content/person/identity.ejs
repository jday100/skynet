<div>
    Hello
</div>
<div>
    <div>
        <div>
            <label>Username:</label>
            <input disabled type="text" value="<%=identity.username%>">
        </div>
    </div>
</div>
<div>
    <div>
        <div>
            <label>Nickname:</label>
            <input disabled type="text" name="nickname" value="<%=identity.nickname%>">
        </div>
        <button id="nickname_button" onclick="">Edit</button>
    </div>
</div>
<div>
    <div>
        <div>
            <label>Location:</label>
        </div>
        <button id="location_button" onclick="location_edit();">Edit</button>
    </div>
    <div id="location_box">
        <div>
            <dl>
                <dd>
                    <div class="region">
                        <label>Continent:</label>
                        <input type="hidden" id="continent_id" value="<%=identity.continent_id%>">
                        <input disabled type="text" id="continent_name" value="">
                    </div>
                    
                    <div class="region">
                        <label>Region:</label>
                        <input type="hidden" id="region_id" value="<%=identity.region_id%>">
                        <input disabled type="text" id="region_name" value="">
                    </div>

                    <div class="region">
                        <label>City:</label>
                        <input type="hidden" id="city_id" value="<%=identity.city_id%>">
                        <input disabled type="text" id="city_name" value="">
                    </div>
                </dd>
            </dl>
        </div>
    </div>
</div>
<div>
    <div>
        <div>
            <label>Intro:</label>
        </div>
        <button id="intro_button" onclick="intro_edit();">Edit</button>
    </div>
</div>
<div>
    <form id="intro_form">
    <input type="hidden" name="test">
    <div>
        <div>
            <textarea disabled id="intro" name="intro" rows="10" cols="80"><%=identity.intro%></textarea>
        </div>
    </div>
    </form>
</div>